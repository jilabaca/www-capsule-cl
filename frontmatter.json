{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "authorTwitter",
          "name": "authorTwitter",
          "type": "string"
        },
        {
          "title": "cover",
          "name": "cover",
          "type": "string"
        },
        {
          "title": "coverCaption",
          "name": "coverCaption",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "",
            ""
          ]
        },
        {
          "title": "showFullContent",
          "name": "showFullContent",
          "type": "boolean"
        },
        {
          "title": "readingTime",
          "name": "readingTime",
          "type": "boolean"
        },
        {
          "title": "hideComments",
          "name": "hideComments",
          "type": "boolean"
        },
        {
          "title": "color",
          "name": "color",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "archetypes",
      "path": "[[workspace]]/archetypes"
    },
    {
      "title": "re-terminal",
      "path": "[[workspace]]/themes/re-terminal"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "archetypes",
      "path": "[[workspace]]/themes/re-terminal/archetypes"
    },
    {
      "title": "content",
      "path": "[[workspace]]/themes/re-terminal/demoSite/content"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/themes/re-terminal/demoSite/content/posts"
    }
  ],
  "frontMatter.git.enabled": true
}